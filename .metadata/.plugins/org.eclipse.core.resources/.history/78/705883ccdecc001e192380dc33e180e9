package application;

import java.time.Instant;
import java.time.LocalDate;
import java.time.LocalDateTime;
import java.util.Locale;
import java.util.Scanner;


public class program {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
      LocalDate d04 = LocalDate.parse("2024-07-20");
      LocalDateTime d05 = LocalDateTime.parse("2022-0720T01:30:26");
      Instant d06 = Instant.parse("2024-02-20T01:30:26Z");
     
     LocalDateTime pastWeekLocaleDateTime = d05.minusDays(7);
     LocalDateTime nextWeekLocalDateTime = d05.plusDays(7);
  
 	 System.out.println("pastWeekLocaleDateTime" + pastWeekLocaleDateTime);
 	 System.out.println("nextWeekLocalDateTime" + nextWeekLocalDateTime);
		sc.close(); 
 }
}
